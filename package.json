{
  "name": "react_client_starter_app",
  "version": "1.0.0",
  "description": "Client only React Starter App",
  "author": "atomicjolt",
  "license": "MIT",
  "homepage": "https://github.com/atomicjolt/react_client_starter_app",
  "scripts": {
    "test": "./tests.sh",
    "test_debug": "./tests_debug.sh",
    "atomic-reactor-build": "node ../build.js",
    "atomic-reactor-dev": "node ../webpack.dev.js",
    "atomic-reactor-server": "node ../server.js",
    "watch": "cd ./client && for dir in apps/*; do cd \"$dir\" || continue; yarn watch; cd ..; cd ..; done",
    "hot": "node ../webpack.dev.js --configDir=../react_client_starter_app/client/config --hot",
    "dev": "node ../webpack.dev.js --configDir=../react_client_starter_app/client/config",
    "hot_pack": "yarn atomic-reactor-dev --hotPack --configDir=../react_client_starter_app/client/config",
    "hot_lint": "yarn atomic-reactor-dev --hotPack --lint --configDir=../react_client_starter_app/client/config",
    "hot_debug": "node --nolazy --inspect-brk=9229 ./node_modules/atomic-webpack/webpack.hot.js --configDir=../react_client_starter_app/client/config",
    "hot_pack_debug": "node --nolazy --inspect-brk=9229 ./node_modules/atomic-webpack/webpack.hot.js --hotPack --configDir=../react_client_starter_app/client/config",
    "live": "yarn atomic-reactor-server --configDir=../react_client_starter_app/client/config",
    "build_dev": "yarn atomic-reactor-build --configDir=../react_client_starter_app/client/config",
    "build_dev_pack": "yarn atomic-reactor-build --onlyPack --configDir=../react_client_starter_app/client/config",
    "build": "yarn atomic-reactor-build --release --configDir=../react_client_starter_app/client/config",
    "build_pack": "yarn atomic-reactor-build --release --onlyPack --configDir=../react_client_starter_app/client/config",
    "ball": "yarn atomic-reactor-build --release --configDir=../react_client_starter_app/client/config && ./node_modules/s3-website/s3-website.js create",
    "release": "yarn atomic-reactor-build --release --configDir=../react_client_starter_app/client/config && ./node_modules/s3-website/s3-website.js deploy",
    "lint": "cd ./client && eslint js",
    "nuke": "rm -rf node_modules && cd ./client && yarn nuke",
    "inspect_reactor": "\"${EDITOR:-vi}\" ./node_modules/atomic-webpack",
    "inspect_fuel": "\"${EDITOR:-vi}\" ./node_modules/atomic-fuel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atomicjolt/react_client_starter_app"
  },
  "keywords": [
    "react"
  ],
  "dependencies": {
    "del": "^3.0.0"
  },
  "devDependencies": {
    "atomic-reactor": "^3.2.3"
  },
  "private": true,
  "workspaces": [
    "client",
    "client/apps/*"
  ]
}
